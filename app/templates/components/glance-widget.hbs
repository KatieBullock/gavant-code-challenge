<Page::Content>
  <div class="col-lg-9">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title">
          At a Glance
        </h5>
        <h5>
          {{model.length}}
        </h5>
        <p class="text-muted">
          Customers in pipeline
        </p>
        {{#each model as |customer|}}
        <h5>
          {{customer.budget}}
        </h5>
        {{/each}}
        <h5>{{this.totalBudget}}</h5>
        <p>
          Total forecasted budget
        </p>
      </div>
    </div>
  </div>
</Page::Content>